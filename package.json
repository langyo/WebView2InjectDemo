{
  "private": true,
  "description": "基于 Edge 与 .NET 的 OneNet 物联网设备仪表板实验项目",
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp clean && npm run build-ts && npm run build-cs",
    "build-ts": "gulp build",
    "build-cs": "dotnet build ./builder.csproj --nologo -r win-x64 --self-contained=true -o ./bin/",
    "publish": "cross-env NODE_ENV=production gulp clean && npm run publish-ts && npm run publish-cs && gulp postPublish",
    "publish-ts": "gulp build",
    "publish-cs": "dotnet publish ./builder.csproj --nologo -r win-x64 --self-contained=true /p:PublishSingleFile=true -o ./dist/publish/win-unpacked/bin/",
    "start": "./bin/builder.exe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langyo/OneNetSerialDashboardDemoEnterprise.git"
  },
  "author": "langyo",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/langyo/OneNetSerialDashboardDemoEnterprise/issues"
  },
  "homepage": "https://github.com/langyo/OneNetSerialDashboardDemoEnterprise#readme",
  "dependencies": {
    "core-js": "^3.6.5",
    "shortid": "^2.2.16",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@types/gulp": "^4.0.9",
    "@types/node": "^17.0.21",
    "@vue/compiler-sfc": "^3.2.31",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.1.5",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.70.0"
  }
}
